def lastIndex(arr,x):
    l=len(arr)
    if l==0:
        return -1
    smallerlist=arr[1:]
    smallerlistoutput=lastIndex(smallerlist,x)
    
    if smallerlistoutput != -1:
        return smallerlistoutput +1
    
    else:
        if arr[0]==x:
            return 0
        else:
            return -0
        

n=int(input())
arr=list(int(i) for i in input().split())
x=int(input())
print(lastIndex(arr,x))
